<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.viewpager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.csei.inspect.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.csei.inspect.ScanCardActivity">  
        </activity>
        <activity
            android:name="com.csei.inspect.PeopleValidateActivity">  
        </activity>
        <activity
            android:name="com.csei.inspect.TagValidateActivity">  
        </activity>
        <activity
            android:name="com.csei.inspect.StartInspectActivity">  
        </activity>
        <activity
            android:name="com.csei.inspect.WelcomeActivity">  
        </activity>
       <service android:name="com.example.service.RFIDService" ></service>
    </application>
      <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/> 
      <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/> 
</manifest>